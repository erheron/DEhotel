<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" focusTraversable="true" prefHeight="288.0" prefWidth="446.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
   <children>
      <Button fx:id="loginB" focusTraversable="false" layoutX="175.0" layoutY="216.0" mnemonicParsing="false" onAction="#loginBpressed" prefHeight="26.0" prefWidth="95.0" text="Log in" />
      <Label focusTraversable="false" layoutX="139.0" layoutY="58.0" prefHeight="16.0" prefWidth="171.0" text="Hello! Log in to use app" textAlignment="CENTER" />
      <PasswordField fx:id="passwordF" focusTraversable="false" layoutX="137.0" layoutY="156.0" onKeyPressed="#passwordFkPressed" promptText="Enter password" />
      <TextField fx:id="loginF" focusTraversable="false" layoutX="137.0" layoutY="107.0" onKeyPressed="#loginFkPressed" promptText="Enter login" />
   </children>
</AnchorPane>
