<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AdminController">
   <children>
      <Button fx:id="penaltyButton" layoutX="14.0" layoutY="40.0" mnemonicParsing="false" onAction="#penaltyButtonAction" prefHeight="26.0" prefWidth="76.0" text="Penalty" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="40.0" />
      <Button fx:id="oneMoreButton" layoutX="380.0" layoutY="172.0" mnemonicParsing="false" onAction="#oneMoreButtonAction" text="Insert one more" AnchorPane.leftAnchor="380.0" AnchorPane.topAnchor="172.0" />
      <TextField fx:id="reservationTF" layoutX="147.0" layoutY="40.0" onKeyPressed="#reservationTFOnKeyPressed" promptText="type reservation id A/B" />
      <Button fx:id="submitButton" layoutX="141.0" layoutY="172.0" mnemonicParsing="false" onAction="#submitButtonAction" prefHeight="26.0" prefWidth="202.0" text="Submit" />
      <MenuButton fx:id="selectMenu" focusTraversable="false" layoutX="348.0" layoutY="40.0" mnemonicParsing="false" text="Select equipment">
      </MenuButton>
   </children>
</AnchorPane>
