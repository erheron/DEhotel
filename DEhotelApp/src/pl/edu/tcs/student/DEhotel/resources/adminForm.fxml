<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AdminController">
   <children>
      <Button fx:id="penaltyButton" layoutX="14.0" layoutY="40.0" mnemonicParsing="false" text="Penalty" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="40.0" />
      <Button fx:id="insertONeMoreButton" layoutX="15.0" layoutY="83.0" mnemonicParsing="false" text="Insert one more" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="83.0" />
      <TextField fx:id="reservationTF" layoutX="147.0" layoutY="40.0" onKeyPressed="#reservationTFOnKeyPressed" promptText="type reservation id" />
      <Button fx:id="submitButton" layoutX="147.0" layoutY="133.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="171.0" text="Submit" />
      <MenuButton fx:id="selectMenu" focusTraversable="false" layoutX="147.0" layoutY="83.0" mnemonicParsing="false" text="Select equipment">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
      </MenuButton>
      <TextField fx:id="totalCashTF" layoutX="371.0" layoutY="40.0" promptText="total cash amount" />
   </children>
</AnchorPane>
